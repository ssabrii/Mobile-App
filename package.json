{
  "name": "Mobile-App",
  "version": "1.0.21",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "detox": "^9.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "jest-react-native": "^18.0.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "16.2.0",
    "reactotron-react-native": "^2.1.4",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js",
    "lint": "./node_modules/.bin/eslint ."
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "test-runner": "mocha",
    "specs": "e2e",
    "runner-config": "e2e/mocha.opts",
    "configurations": {
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/MobileApp.app",
        "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -project ios/MobileApp.xcodeproj -UseNewBuildSystem=NO -scheme MobileApp -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/MobileApp.app",
        "build": "xcodebuild -project ios/MobileApp.xcodeproj -UseNewBuildSystem=NO -scheme MobileApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "ios.sim.debugXS": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/MobileApp.app",
        "build": "xcodebuild -project ios/MobileApp.xcodeproj -UseNewBuildSystem=NO -scheme MobileApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone XS"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "pushd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "pushd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && popd",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      }
    }
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^1.0.0",
    "apisauce": "^0.15.2",
    "axios": "^0.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "base-64": "^0.1.0",
    "buffer": "^5.1.0",
    "card-validator": "^5.0.0",
    "core-decorators": "^0.20.0",
    "ethers": "git://github.com/yuri930/ethers.js.git",
    "immutability-helper": "^2.7.1",
    "immutable": "^3.8.2",
    "javascript-time-ago": "^1.0.30",
    "locktrip-service-layer": "^1.0.45",
    "lodash": "^4.17.10",
    "missing": "0.0.1",
    "moment": "^2.22.1",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "react": "16.2.0",
    "react-mixin": "^2.0.2",
    "react-native": "^0.55.4",
    "react-native-background-fetch": "^2.0.8",
    "react-native-cardview": "^1.1.5",
    "react-native-checkbox": "^2.0.0",
    "react-native-customisable-switch": "git://github.com/baderahmed/react-native-customisable-switch.git#118fe5aa51150b8c471c081a23ba38087aa210c6",
    "react-native-dash": "0.0.8",
    "react-native-easy-toast": "^1.1.0",
    "react-native-fast-image": "^5.1.1",
    "react-native-fbsdk": "^0.8.0",
    "react-native-fontawesome": "^5.7.0",
    "react-native-hyperlink": "^0.0.14",
    "react-native-image-picker": "^0.27.1",
    "react-native-image-resizer": "^1.0.0",
    "react-native-image-zoom-viewer": "^2.2.12",
    "react-native-indicators": "^0.13.0",
    "react-native-iphone-x-helper": "^1.0.2",
    "react-native-keyboard-aware-scroll-view": "^0.7.2",
    "react-native-maps": "^0.22.1",
    "react-native-masked-text": "^1.7.2",
    "react-native-modal-datetime-picker": "^5.1.0",
    "react-native-phone-input": "^0.2.1",
    "react-native-picker-select": "^3.0.1",
    "react-native-remote-svg": "^1.3.0",
    "react-native-simple-toast": "0.0.8",
    "react-native-smart-splash-screen": "git://github.com/rgdalessandro/react-native-smart-splash-screen.git#a803be27fececa72f446cb16b4b2d2dd6c5c09e4",
    "react-native-tab-view": "^1.3.1",
    "react-native-typography": "^1.3.0",
    "react-native-uuid-generator": "^4.0.0",
    "react-native-vector-icons": "^6.0.2",
    "react-navigation": "^2.18.3",
