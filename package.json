    "react-redux": "^5.0.7",
    "react-simple-dropdown": "^3.2.0",
    "react-stomp": "^3.2.0",
    "react-switch": "^2.3.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.2.0",
    "rfc4648": "^1.1.0",
    "shortid": "^2.2.8",
    "sockjs-client": "^1.1.4",
    "stomp-websocket-js": "^1.0.0",
    "stompjs": "^2.3.3",
    "stream": "0.0.2",
    "utf8": "^3.0.0"
  }
}
